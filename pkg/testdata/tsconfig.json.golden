{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "baseUrl": "./src",
        "rootDir": "src",
        "outDir": "dist",
        "resolveJsonModule": true,
        "target": "es6",
        "lib": [
            "esnext",
            "es2018",
            "dom"
        ]
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
    ]
}
